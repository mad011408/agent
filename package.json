{
  "name": "ultra-advanced-autonomous-ai-agent-system",
  "version": "1.0.0",
  "description": "ðŸŒŸ World's Most Advanced Autonomous AI Agent System with Multi-Provider Support",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:ai-engine\"",
    "dev:frontend": "cd FRONTEND-ECOSYSTEM/ai-powered-ui && npm run dev",
    "dev:backend": "cd BACKEND-MICROSERVICES-ECOSYSTEM/api-gateway-service && npm run dev",
    "dev:ai-engine": "cd AI-ML-DEEP-LEARNING-ENGINE && python -m uvicorn api.main:app --reload --port 8001",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd FRONTEND-ECOSYSTEM/ai-powered-ui && npm run build",
    "build:backend": "npm run build:services",
    "build:services": "echo 'Building microservices...'",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "k8s:deploy": "kubectl apply -f INFRASTRUCTURE-AS-CODE/kubernetes/",
    "setup": "bash SCRIPTS-AUTOMATION/setup.sh",
    "migrate": "bash SCRIPTS-AUTOMATION/migrate.sh",
    "seed": "node SCRIPTS-AUTOMATION/seed-data.js"
  },
  "keywords": [
    "ai",
    "autonomous-agents",
    "machine-learning",
    "microservices",
    "multi-agent-system",
    "llm",
    "vector-database",
    "knowledge-graph",
    "auto-scaling",
    "self-healing"
  ],
  "author": "AI Agent System Team",
  "license": "MIT",
  "workspaces": [
    "FRONTEND-ECOSYSTEM/ai-powered-ui",
    "BACKEND-MICROSERVICES-ECOSYSTEM/*"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@pinecone-database/pinecone": "^2.0.0",
    "@tensorflow/tfjs-node": "^4.17.0",
    "axios": "^1.6.7",
    "bullmq": "^5.1.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "langchain": "^0.1.25",
    "mongoose": "^8.1.1",
    "openai": "^4.28.0",
    "pg": "^8.11.3",
    "socket.io": "^4.6.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
}