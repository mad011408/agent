{
  "environment": "development",
  "server": {
    "port": 3000,
    "apiPort": 8000,
    "pythonApiPort": 8001
  },
  "database": {
    "postgres": {
      "host": "localhost",
      "port": 5432,
      "database": "ai_agent_system_dev",
      "pool": {
        "min": 2,
        "max": 10
      }
    },
    "mongodb": {
      "uri": "mongodb://localhost:27017/ai_agent_system_dev"
    },
    "redis": {
      "host": "localhost",
      "port": 6379,
      "ttl": 3600
    }
  },
  "ai": {
    "defaultProvider": "nvidia",
    "maxTokens": 4096,
    "temperature": 0.7,
    "streamEnabled": true,
    "cachingEnabled": true
  },
  "security": {
    "jwtExpiresIn": "24h",
    "bcryptRounds": 10,
    "rateLimitWindow": 60000,
    "rateLimitMaxRequests": 100
  },
  "logging": {
    "level": "debug",
    "format": "json",
    "colorize": true
  },
  "features": {
    "autoScaling": false,
    "selfHealing": true,
    "multiAgent": true,
    "knowledgeGraph": true
  }
}